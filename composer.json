{
    "name": "gos/web-socket-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Web Socket Bundle",
    "keywords": ["Web Socket Bundle", "Websocket", "WAMP", "IO", "Ratchet"],
    "homepage": "https://github.com/GeniusesOfSymfony/WebSocketBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Dare",
            "email": "jeremy.d.dare@gmail.com"
        },
        {
            "name": "Johann Saunier",
            "email": "johann_27@hotmail.fr"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "cboden/ratchet": "^0.5",
        "gos/pubsub-router-bundle": "^2.2 || ^3.0",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "react/event-loop": "^1.2",
        "react/socket": "^1.9",
        "symfony/config": "^5.3",
        "symfony/console": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/deprecation-contracts": "^2.1 || ^3.0",
        "symfony/event-dispatcher": "^5.3",
        "symfony/http-foundation": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symfony/security-core": "^5.3",
        "symfony/string": "^5.3"
    },
    "require-dev": {
        "doctrine/dbal": "^2.13.1 || ^3.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "1.2.0",
        "phpstan/phpstan-phpunit": "1.0.0",
        "phpstan/phpstan-symfony": "1.0.1",
        "phpunit/phpunit": "^9.5",
        "symfony/cache": "^5.3",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/twig-bundle": "^5.3"
    },
    "conflict": {
        "doctrine/dbal": "<2.13.1 || >=3.0,<3.1",
        "symfony/cache": "<5.3",
        "symfony/monolog-bundle": "<3.0",
        "symfony/twig-bundle": "<5.3",
        "twig/twig": "<1.36 || >=2.0,<2.6"
    },
    "autoload": {
        "psr-4": { "Gos\\Bundle\\WebSocketBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Gos\\Bundle\\WebSocketBundle\\Tests\\": "tests/" }
    },
    "suggest": {
        "ext-pcntl": "* to handle process signals",
        "ext-pdo": "* to use PDO ping services",
        "doctrine/dbal": "to use Doctrine ping services",
        "symfony/cache": "to use symfony/cache as an authentication storage driver"
    },
    "minimum-stability": "dev"
}
